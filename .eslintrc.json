{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},

	"extends": "eslint:recommended",

	"globals": {
		"define": false,
		"moment": false,
		"JSPath": false
	},

	"rules": {
		"no-mixed-spaces-and-tabs": 0,

		"no-unused-vars": [
			1,
			{"vars": "local", "args": "after-used"}
		],

		"quotes": [
			2,
			"single"
		]
	}
}
